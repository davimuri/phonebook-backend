    "deploy:full": "npm run build:ui && git add . && git commit -m uibuild && npm run deploy",
    "logs:prod": "heroku logs --tail"
  },
  "keywords": [
    "phonebook"
  ],
  "author": "David Murillo",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/davimuri/phonebook-backend.git"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "mongoose": "^5.7.7",
    "morgan": "^1.9.1"
  },
  "devDependencies": {
    "nodemon": "^1.19.4"
  }
}
